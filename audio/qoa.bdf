{
  meta: {
    bdf: "0.4",
    name: "Quite Okay Audio (QOA) data",
    mime: ["audio/x-qoa"],
    exts: [".qoa"]
  },
  binary: [
    { type: magic, match: "qoaf" },
    { id: samples, type: uint32, endian: "big", name: "Samples per channel" },
    { id: frame,
      name: "First frame",
      type: struct, // array of struct[ceil(samples / (256 * 20))],
      endian: "big",
      fields: [
        { type: uint8,  id: numChannels,  name: "No. of channels" },
        { type: uint24, id: sampleRate,   name: "Sample rate (hz)" },
        { type: uint16, id: frameSamples, name: "Samples per channel in frame" },
        { type: uint16, id: frameSize,    name: "Frame size" },
        { type: byte[frameSize - 8], 
          id: frameData, 
          name: "Frame data" 
        }
      ]
    }
  ]
}
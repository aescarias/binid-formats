{
  meta: {
    bdf: "0.3",
    name: "Quite Okay Audio (QOA) data",
    mime: ["audio/x-qoa"],
    exts: [".qoa"]
  },
  binary: [
    { type: magic, match: "qoaf" },
    { id: samples, type: uint32, endian: "big", name: "Samples per channel" },
    { id: frame,
      name: "First frame",
      type: struct, // struct[ceil(samples / (256 * 20))],
      endian: "big",
      fields: [
        { id: numChannels, type: uint8, name: "No. of channels" },
        // rudimentary uint24 parsing
        { type: uint8, id: _srb1 }, { type: uint8, id: _srb2 }, { type: uint8, id: _srb3 }, 
        { type: var, id: sampleRate, 
          value: _srb1<<16 | _srb2<<8 | _srb3, 
          name: "Sample rate (hz)" 
        },
        { id: frameSamples, type: uint16, name: "Samples per channel in frame" },
        { id: frameSize, type: uint16, name: "Frame size" },
        { id: frameData, type: byte[frameSize - 8] }
      ]
    }
  ]
}